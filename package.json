{
	"name": "http-requests-snippets-extension",
	"displayName": "Http Requests Snippets",
	"publisher": "Heigen007",
	"description": "VS Code extension for http requests snippets with Axios, Fetch and Superagent",
	"version": "0.0.1",
	"categories": ["Snippets"],
	"engines": {
		"vscode": "^1.59.0"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/username/repository.git"
	},
	"activationEvents": [
		"onCommand:myext.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
		  {
			"language": "javascript",
			"path": "./snippets.json"
		  }
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
